<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:toolUi="clr-namespace:Examples.ToolUi"
        mc:Ignorable="d"
        d:DesignWidth="800"
        d:DesignHeight="450"
        x:Class="Examples.ToolUi.MainWindow"
        Title="Tools UI"
        d:DataContext="{d:DesignInstance toolUi:MainWindow}">
    <ListBox x:Name="Output"
             BorderThickness="0"
             Items="{Binding OutputList}"
             IsEnabled="False"
             Background="{DynamicResource ThemeAlternativeBackgroundBrush}" />
</Window>